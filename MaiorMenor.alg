algoritmo "semnome"
// Função :
// Autor :
// Data : 25/05/2022
// Seção de Declarações
var
num1, num2, maior: inteiro
inicio
EscrevaL("Digite os valores: ")
Leia(num1, num2)

Se (num1 > num2) ENTAO

   maior <- num1

   SENAO

   maior <- num2

FIMSE

Escreva("o maior número: ", maior)

fimalgoritmo
